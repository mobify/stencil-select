<div class="c-select {class} {?label}c--inner-label{/label}" data-adaptivejs-component="stencil-select">
    {?element}
        {element|s}
    {:else}
        {?label}
            <label class="c-select__label">{label}</label>
            <span class="c-select__value"></span>
        {/label}

        <select {?id}id="{id}"{/id} {?name}name="{name}"{/name} {?disabled}disabled{/disabled} {?required}required{/required}>
            {body}
        </select>
    {/element}
</div>
